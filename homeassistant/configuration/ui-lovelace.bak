title: Williamson Ave
views:
  - badges:
      - entity: binary_sensor.home_occupied
      - entity: person.bradley
      - entity: person.nirupa
    cards:
      - card:
          show_header_toggle: false
          tap_action:
            action: toggle
          title: Doors open + locks
          type: entities
        entities:
          - entity: cover.garage_door
            secondary_info: last-changed
          - entity: lock.front_door
            secondary_info: last-changed
          - entity: binary_sensor.front_door_open
            secondary_info: last-changed
          - entity: binary_sensor.aeotec_zw112_door_window_sensor_6_sensor
            secondary_info: last-changed
          - entity: binary_sensor.aeotec_zw112_door_window_sensor_6_sensor_2
            secondary_info: last-changed
        show_empty: false
        show_header_toggle: false
        state_filter:
          - open
          - locked
        type: entity-filter
      - card:
          show_header_toggle: true
          title: Lights + switches on
          type: entities
        entities:
          - switch.aeotec_zw132_dual_nano_switch_switch_13
          - switch.aeotec_zw132_dual_nano_switch_switch_14
          - switch.switch_7
          - switch.aeotec_zw132_dual_nano_switch_switch_18
          - switch.aeotec_zw132_dual_nano_switch_switch_17
          - switch.aeotec_zw132_dual_nano_switch_switch_16
          - switch.aeotec_zw132_dual_nano_switch_switch_15
          - switch.aeotec_zw132_dual_nano_switch_switch_21
          - switch.aeotec_zw132_dual_nano_switch_switch_22
          - switch.aeotec_zw116_nano_switch_switch_3
          - light.level
          - switch.aeotec_zw132_dual_nano_switch_switch_20
          - switch.aeotec_zw132_dual_nano_switch_switch_19
          - switch.aeotec_zw116_nano_switch_switch
          - switch.aeotec_zw116_nano_switch_switch_2
          - switch.smart_switch
          - switch.aeon_labs_zw132_dual_nano_switch_switch_2
          - switch.aeon_labs_zw132_dual_nano_switch_switch
        show_empty: false
        state_filter:
          - 'on'
        type: entity-filter
      - card:
          show_header_toggle: false
          title: Heat pumps on
          type: entities
        entities:
          - climate.living_room_heat_pump
          - climate.bedroom_heat_pump
        show_empty: false
        state_filter:
          - Cool
          - Auto
          - Heat
          - Dry
          - Fan
        type: entity-filter
      - entities:
          - entity: switch.vehicle_gate_opener
          - entity: lock.front_door
          - entity: input_boolean.guest_mode
          - entity: binary_sensor.front_door_open
          - entity: switch.aeon_labs_zw132_dual_nano_switch_switch_2
          - entity: switch.aeon_labs_zw132_dual_nano_switch_switch
          - entity: binary_sensor.entrance
        entity: binary_sensor.front_door_open
        hold_action:
          action: call-service
          service: switch.toggle
          service_data:
            entity_id: switch.vehicle_gate_opener
        show_header_toggle: false
        state_image:
          'off': /local/front_door_closed.jpg
          'on': /local/front_door_open.jpg
          unavailable: /local/front_door_closed.jpg
        title: Entrance
        type: picture-glance
      - camera_image: camera.garage_camera
        entities:
          - entity: binary_sensor.garage_camera_motion_sensor
          - entity: cover.garage_door
        hold_action:
          action: call-service
          service: cover.open_cover
          service_data:
            entity_id: cover.garage_door
        show_header_toggle: false
        title: Garage
        type: picture-glance
      - entities:
          - entity: switch.aeotec_zw116_nano_switch_switch_2
          - entity: binary_sensor.aeotec_zw112_door_window_sensor_6_sensor
          - entity: binary_sensor.aeotec_zw112_door_window_sensor_6_sensor_2
          - entity: light.level
          - entity: binary_sensor.living_room
        entity: group.living
        show_header_toggle: false
        state_image:
          'off': /local/living_room_off.jpg
          'on': /local/living_room.jpg
        title: Living room
        type: picture-glance
      - entities:
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_13
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_14
          - entity: media_player.bedroom_home_speaker
        entity: group.bedroom
        state_image:
          'off': /local/bedroom_off.jpg
          'on': /local/bedroom.jpg
        title: Bedroom
        type: picture-glance
      - entities:
          - entity: media_player.kitchen_speaker
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_19
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_20
          - entity: switch.aeotec_zw116_nano_switch_switch
        entity: group.kitchen
        show_header_toggle: false
        state_image:
          'off': /local/kitchen_off.jpg
          'on': /local/kitchen.jpg
        title: Kitchen
        type: picture-glance
      - entities:
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_17
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_18
          - entity: switch.switch_7
            icon: 'mdi:fan'
        entity: group.bathroom_lights
        state_image:
          'off': /local/bathroom_off.jpg
          'on': /local/bathroom.jpg
        title: Bathroom
        type: picture-glance
      - entities:
          - entity: binary_sensor.aeotec_zw100_multisensor_6_sensor
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_15
          - entity: switch.aeotec_zw132_dual_nano_switch_switch_16
          - entity: switch.smart_switch
        entity: group.upstairs
        show_header_toggle: true
        state_image:
          'off': /local/upstairs_off.jpg
          'on': /local/upstairs.jpg
        title: Upstairs
        type: picture-glance
    icon: 'mdi:home'
    path: home
    title: Overview
  - badges: []
    cards:
      - cards:
          - entity: alarm_control_panel.alarm
            states:
              - arm_home
              - arm_away
            type: alarm-panel
          - entities:
              - alarm_control_panel.alarm
            hours_to_show: 12
            refresh_interval: 10
            type: history-graph
        type: vertical-stack
    icon: 'mdi:alarm-bell'
    path: alarm
    title: Alarm
  - badges:
      - camera.entrance_phone
      - camera.garage_camera
      - camera.cube_camera
      - camera.front_door_doorbell
    cards:
      - entities:
          - entity: binary_sensor.home_occupied
            secondary_info: last-changed
          - entity: binary_sensor.entrance
            secondary_info: last-changed
          - entity: binary_sensor.garage_camera_motion_sensor
            secondary_info: last-changed
          - entity: binary_sensor.guest_bedroom
            secondary_info: last-changed
          - entity: binary_sensor.living_room
            secondary_info: last-changed
          - entity: binary_sensor.aeotec_zw100_multisensor_6_sensor
            secondary_info: last-changed
        show_header_toggle: false
        title: Motion sensors
        type: entities
      - entities:
          - label: Entrance
            type: section
          - camera.entrance_phone
          - icon: 'mdi:record-rec'
            name: Entrance camera history
            type: weblink
            url: 'http://media-hub:881/'
          - label: Garage
            type: section
          - camera.garage_camera
          - switch.garage_camera_night_mode
          - cover.garage_camera_move_left_right
          - cover.garage_camera_move_up_down
          - icon: 'mdi:record-rec'
            name: Garage camera history
            type: weblink
            url: 'http://media-hub:882/'
          - label: Bedroom
            type: section
          - camera.cube_camera
          - switch.cube_cam_night_mode
          - icon: 'mdi:record-rec'
            name: Bedroom camera history
            type: weblink
            url: 'http://media-hub:883/'
        show_header_toggle: false
        title: Security cameras
        type: entities
    icon: 'mdi:camcorder'
    path: cameras
    title: Cameras and Motion
  - badges: []
    cards:
      - cards:
          - entity: sensor.entrance_temperature
            max: 40
            name: Outside
            severity:
              green: 15
              red: 25
              yellow: 20
            type: gauge
          - entity: sensor.aeotec_zw100_multisensor_6_temperature
            max: 40
            name: Upstairs
            severity:
              green: 15
              red: 27
              yellow: 20
            type: gauge
        type: horizontal-stack
      - entities:
          - label: Living areas
            type: section
          - entity: climate.living_room_heatpump
          - entity: climate.bedroom_heatpump
          - entity: script.bedroom_heatpump_off_delayed
          - entity: switch.smart_switch
            secondary_info: last-changed
          - label: Entrance
            type: section
          - entity: sensor.entrance_temperature
          - entity: sensor.entrance_humidity
        show_header_toggle: false
        title: Cooling + heating
        type: entities
      - entity: weather.openweathermap
        type: weather-forecast
    icon: 'mdi:temperature-celsius'
    panel: false
    path: climate
    title: Climate
